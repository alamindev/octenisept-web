{
  "name": "gesundleben-webspecial",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development BROWSERSLIST_ENV=development BROWSERSLIST_CONFIG=.config/.browserslistrc webpack serve --config .config/webpack/webpack.dev.js",
    "prod": "npm run build",
    "build": "cross-env NODE_ENV=production BROWSERSLIST_ENV=production BROWSERSLIST_CONFIG=.config/.browserslistrc webpack --config .config/webpack/webpack.prod.js && rm dist/index.html && cross-env NODE_ENV=production node .config/scripts/append.js",
    "preview": "cross-env NODE_ENV=preview BROWSERSLIST_ENV=production BROWSERSLIST_CONFIG=.config/.browserslistrc webpack --config .config/webpack/webpack.prod.js && copyfiles -u 1 assets/**/* dist && cross-env NODE_ENV=preview node .config/scripts/append.js",
    "lint:js": "eslint src/**/*.js --config .config/.eslintrc.js --fix",
    "lint:css": "stylelint src/**/*.css --config .config/.stylelintrc.js --fix",
    "lint": "run-p lint:*",
    "format": "prettier --config .config/.prettierrc.js --ignore-path .config/.prettierignore --write .",
    "publish:script": "node .config/scripts/publish.js",
    "publish": "npm-run-all format build publish:script"
  },
  "dependencies": {
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "autoprefixer": "^10.4.4",
    "axios": "^0.26.1",
    "babel-loader": "^8.2.4",
    "chalk": "^4.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.12.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.1",
    "glob-all": "^3.3.0",
    "html-minimizer-webpack-plugin": "^3.5.0",
    "inquirer": "^8.2.2",
    "js-yaml": "^4.1.0",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^6.2.1",
    "postcss-prefix-selector": "^1.15.0",
    "prettier": "^2.6.2",
    "progress-webpack-plugin": "^1.0.16",
    "require-context": "^1.1.0",
    "style-loader": "^3.3.1",
    "stylelint": "^15.10.1",
    "stylelint-config-recommended": "^13.0.0",
    "tailwindcss-debug-screens": "^2.2.1",
    "twig-html-loader": "^0.1.9",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0"
  }
}
